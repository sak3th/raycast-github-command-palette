{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-command-palette",
  "title": "Command Palette",
  "description": "Command palette for Github",
  "icon": "icon.png",
  "author": "sak3th",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Github",
      "description": "Work with issues, pull requests, workflows, search repositories and stay on top of notifications",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "title": "Personal Access Token",
      "description": "Personal Access Token for GitHub",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "@raycast/utils": "^1.4.8",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.185",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
